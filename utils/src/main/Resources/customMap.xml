<?xml version="1.0" encoding="UTF-8"?>
<config>
    <version>2</version>
    <type>Vector</type>
    <tiles>/tile/{L}/{C}/{R}.pbf</tiles>
    <initialExtent>
        <xmin>502167.1699999999</xmin>
        <ymin>3357653.5</ymin>
        <xmax>524017.6799999997</xmax>
        <ymax>3337913.789999999</ymax>
    </initialExtent>
    <fullExtent>
        <xmin>502167.1699999999</xmin>
        <ymin>3357653.5</ymin>
        <xmax>524017.6799999997</xmax>
        <ymax>3337913.789999999</ymax>
    </fullExtent>
    <inputRange>
        <xmin>502167.1699999999</xmin>
        <ymin>3357653.5</ymin>
        <xmax>524017.6799999997</xmax>
        <ymax>3337913.789999999</ymax>
    </inputRange>
    <!--瓦片信息-->
    <tileInfo>
        <originPoint>
            <x>-5466274.966255</x>
            <y>10345174.966255</y>
        </originPoint>
        <baseResolution>8669.8840064346805</baseResolution>
        <tileSize>256</tileSize>
        <dpi>96</dpi>
        <format>pbf</format>
    </tileInfo>
    <!--数据源配置信息-->
    <dataSources>
        <dataSource>
            <id>postgre30</id>
            <name>内网PG库</name>
            <!--数据源名称 -->
            <type>databaseSource</type>
            <subtype>nav_network</subtype>
            <description>
                <driverClassName>org.postgresql.Driver</driverClassName>
                <url>jdbc:postgresql://10.136.34.30:5432/gisdb</url>
                <userName>gis_usr</userName>
                <passWord>gis_usr</passWord>
            </description>
        </dataSource>
        <dataSource>
            <id>postgre30</id>
            <name>外网PG库</name>
            <!--数据源名称 -->
            <type>databaseSource</type>
            <subtype>public</subtype>
            <description>
                <driverClassName>org.postgresql.Driver</driverClassName>
                <url>jdbc:postgresql://192.168.35.127:5432/test_gis</url>
                <userName>postgres</userName>
                <passWord>abc123</passWord>
            </description>
        </dataSource>
    </dataSources>
    <!--图层配置信息-->
    <layers>
        <layer>
            <id>0</id>
            <dataSourceID>postgre30</dataSourceID>
            <layerName>hz_building</layerName>
            <wkid>2385</wkid>
            <minScale>66668888</minScale>
            <maxScale>1</maxScale>
            <dataDesc>select objectid, id, layer_name, st_astext(geom) geom from nav_network.building_area_copy p where
                st_intersects(st_geomfromtext(?, 2385),p.geom) limit 40000
            </dataDesc>
        </layer>
        <layer>
            <id>1</id>
            <dataSourceID>postgre30</dataSourceID>
            <layerName>nav_poi</layerName>
            <wkid>2385</wkid>
            <minScale>66668888</minScale>
            <maxScale>1</maxScale>
            <dataDesc>select objectid, id, layer_name, st_astext(geom) geom from nav_network.building_area_copy p where
                st_intersects(st_geomfromtext(?, 2385),p.geom) limit 40000
            </dataDesc>
        </layer>
        <layer>
            <id>2</id>
            <dataSourceID>postgre30</dataSourceID>
            <layerName>building_area_copy</layerName>
            <wkid>2385</wkid>
            <minScale>66668888</minScale>
            <maxScale>1</maxScale>
            <dataDesc>select objectid, id, layer_name, st_astext(geom) geom from nav_network.building_area_copy p where
                st_intersects(st_geomfromtext(?, 2385),p.geom) limit 40000
            </dataDesc>
        </layer>
    </layers>
    <outputs>
        <output>
            <id>0</id>
            <type>localfile</type>
            <level>7-10</level>
            <path>F:/pbf/hz_building/</path>
        </output>
        <output>
            <id>1</id>
            <type>hbase</type>
            <level>0-0</level>
            <path>hz_building</path>
        </output>
    </outputs>
</config>
